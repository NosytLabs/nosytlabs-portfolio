<?xml version="1.0" encoding="UTF-8"?>
<svg width="100" height="100" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="nosyt-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#2D0A4F" />
      <stop offset="50%" stop-color="#4C1D95" />
      <stop offset="100%" stop-color="#3B0764" />
    </linearGradient>
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="3" result="blur" />
      <feComposite in="SourceGraphic" in2="blur" operator="over" />
    </filter>
    <clipPath id="rounded-clip">
      <rect x="0" y="0" width="100" height="100" rx="15" />
    </clipPath>
  </defs>

  <!-- Background with gradient and rounded corners -->
  <rect x="0" y="0" width="100" height="100" rx="15" fill="url(#nosyt-gradient)" />

  <!-- Decorative tech pattern -->
  <g opacity="0.15" clip-path="url(#rounded-clip)">
    <path d="M0,20 L100,20" stroke="#fff" stroke-width="0.5" />
    <path d="M0,40 L100,40" stroke="#fff" stroke-width="0.5" />
    <path d="M0,60 L100,60" stroke="#fff" stroke-width="0.5" />
    <path d="M0,80 L100,80" stroke="#fff" stroke-width="0.5" />
    <path d="M20,0 L20,100" stroke="#fff" stroke-width="0.5" />
    <path d="M40,0 L40,100" stroke="#fff" stroke-width="0.5" />
    <path d="M60,0 L60,100" stroke="#fff" stroke-width="0.5" />
    <path d="M80,0 L80,100" stroke="#fff" stroke-width="0.5" />

    <circle cx="20" cy="20" r="3" fill="#fff" />
    <circle cx="60" cy="20" r="3" fill="#fff" />
    <circle cx="20" cy="60" r="3" fill="#fff" />
    <circle cx="60" cy="60" r="3" fill="#fff" />
    <circle cx="40" cy="40" r="3" fill="#fff" />
    <circle cx="80" cy="40" r="3" fill="#fff" />
    <circle cx="40" cy="80" r="3" fill="#fff" />
    <circle cx="80" cy="80" r="3" fill="#fff" />
  </g>

  <!-- Stylized 3D N letter -->
  <g filter="url(#glow)">
    <!-- Main N shape -->
    <path d="M25 20 L25 80 L35 80 L35 40 L65 80 L75 80 L75 20 L65 20 L65 60 L35 20 Z" fill="#fff" />

    <!-- 3D effect for N -->
    <path d="M25 20 L25 80 L35 80 L35 40 L65 80 L75 80 L75 20 L65 20 L65 60 L35 20 Z"
          fill="none"
          stroke="#FF6B00"
          stroke-width="2"
          stroke-linejoin="round" />
  </g>

  <!-- Accent elements -->
  <path d="M75 60 L85 60 L85 80 L75 80 Z" fill="#FF6B00" />

  <!-- Tech dots -->
  <circle cx="15" cy="15" r="2" fill="#FF6B00" />
  <circle cx="15" cy="85" r="2" fill="#FF6B00" />
  <circle cx="85" cy="15" r="2" fill="#FF6B00" />
  <circle cx="85" cy="85" r="2" fill="#FF6B00" />

  <!-- Subtle glow effect -->
  <circle cx="50" cy="50" r="35" fill="none" stroke="#FF6B00" stroke-width="1" opacity="0.3" />
</svg>
